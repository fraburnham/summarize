{
    "manifest_version": 3,
    "name": "summarize",
    "version": "1.0",
    "description": "Don't read stuff",
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "contextMenus"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "action": {
        "default_popup": "popup/index.html"
    }
}
